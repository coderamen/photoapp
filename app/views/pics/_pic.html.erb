
<div class="container" id="">
  <div class="row" style="margin-top: 40px;">
    <% obj.each do |pic| %>
    <div class="col-md-4">
      <div class="panel panel-default" id="item">
        <div class="panel-heading card">
          <%= image_tag pic.photo %>
        </div>
        <div class="panel-body">
          <h2><%= link_to pic.title, pic %></h2>
          </div>
          <div class="panel-footer">
            <div class="pic-meta-details">
              <small>Created by:<%= pic.user.username if pic.user %>,
                <%= time_ago_in_words(pic.created_at) %>
                ago, last updated:
                <%= time_ago_in_words(pic.updated_at) %>
                ago</small>
              </div>
          </div>
    </div>
  </div>
  <% end %>
</div>
